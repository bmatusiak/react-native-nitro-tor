{
  "name": "ReactNativeNitroTorExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios:prebuild": "cd ios && pod install",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain\"",
    "build:ios": "react-native build-ios --scheme ReactNativeNitroTorExample --mode Debug --extra-params \"-sdk iphonesimulator ARCHS=arm64 EXCLUDED_ARCHS=x86_64 CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO\"",
    "build:ios:xcode": "xcodebuild build -workspace ios/ReactNativeNitroTorExample.xcworkspace -scheme ReactNativeNitroTorExample -configuration Debug -sdk iphonesimulator -quiet ARCHS=arm64",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@dr.pogodin/react-native-fs": "^2.36.1",
    "@react-native/new-app-screen": "0.82.1",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "react-native-nitro-tor": "workspace:*",
    "react-native-safe-area-context": "^5.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@craby/devkit": "^0.1.0-rc.1",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
