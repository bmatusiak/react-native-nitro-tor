{
  "name": "react-native-nitro-tor-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios:prebuild": "bundle install && cd ios && bundle exec pod install --repo-update",
    "build:android:release": "cd android && ./gradlew assembleDebug",
    "ios": "yarn ios:prebuild && react-native run-ios",
    "start": "react-native start",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain\"",
    "build:ios": "react-native build-ios --scheme NitroTorExample --mode Debug --extra-params \"-sdk iphonesimulator ARCHS=arm64 EXCLUDED_ARCHS=x86_64 CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO\""
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.3",
    "@dr.pogodin/react-native-fs": "^2.32.0",
    "react": "19.0.0",
    "react-native": "0.79.4",
    "react-native-nitro-modules": "^0.26.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.4",
    "@react-native/metro-config": "0.79.4",
    "@react-native/typescript-config": "0.79.4",
    "react-native-builder-bob": "^0.40.12"
  },
  "engines": {
    "node": ">=18"
  }
}
